---
import BaseHead from "../components/BaseHead.astro";
import Footer from "../components/Footer.astro"
import type { Props as BaseHeadProps } from '../components/BaseHead.astro';
import CreateCopyCodeButtons from "../components/CodeButtons/CreateCopyCodeButtons.astro";
import ThemeProvider from "../components/ThemeToggleButton/ThemeProvider.astro";
import MusicList from '../components/MusicList.astro'
import { getAllRel } from '../lib/api.js';
import GroupList from '../components/GroupList.astro'
import Header from "../components/Header.astro";


export interface Props {
	seo?: BaseHeadProps;
}

const { seo } = Astro.props;

const getAllRels = await getAllRel();
---
<html data-wf-page="601d7e7320ecf00bbf8169fb" data-wf-status="1">
  <head>
    <BaseHead {...seo} />
    <ThemeProvider/>
    <CreateCopyCodeButtons />
    <script>
      import '../components/CodeButtons/copy-code-button'
    </script>
    
  </head>
  <body>
    <Header />
    <div class="page-wrapper">
        <slot />
    </div>
    <Footer />

        <!-- Прелодер -->
        <div style="display:flex;opacity:1" class="loader-main-wrapper"><img src="/images/ico.svg" class="image loader-image">
            <div data-w-id="Div Block 4" class="loader-wrapper">
                <div style="-webkit-transform:translate3d(-100%, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-moz-transform:translate3d(-100%, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-ms-transform:translate3d(-100%, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);transform:translate3d(-100%, 0, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0)"
                    class="loader"></div>
            </div>
        </div>
    
        <script src="./js/jquery-3.5.1.min.dc5e7f18c8.js?site=601d7e7320ecf079f58169fd" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
        <script src="./js/webflow.a0db6c760.js" type="text/javascript"></script>

  </body>
</html>
